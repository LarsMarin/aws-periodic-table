<html lang="en" xml:lang="en" xmlns= "https://www.w3.org/1999/xhtml">
<head>
  <meta name="viewport" content="width=device-width,initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta charset="utf-8">

  {{#title}}<title>{{ title }}</title>{{/title}}
  {{#description}}<meta name="description" content="{{description}}"/>{{/description}}
  {{#website}}<link rel="canonical" href="{{website}}/"/>{{/website}}

  {{ > google }}
  {{ > opengraph }}
  {{ > twitter }}


  <style type="text/css">
    body {
      font-family: 'Yanone Kaffeesatz';
      background-color: #ffffff;
      color: #333;
    }
    
    a {
      text-decoration: none;
      color: #f86b00;
    }
    
    .Container {
    }
    
    .Wrapper {
      width: 85%;
      margin:auto;
    }    
     
    .Grid {
      display: inline-grid;
      grid-template-columns: repeat(19, 4vw);
      grid-template-rows: repeat({{grid_rows}}, 4vw);
      grid-column-gap: .2vw;
      grid-row-gap: .2vw;    
    }
    
    .Title {
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 20px;
      margin-bottom: 10px;
      padding: 20px 0 10px 0;
      width: 100%;
    }

    .Title span {
      color: #f86b00;
      font-size: 2rem;
      font-weight: bold;
      text-align: center;
    }

    .Title img {
        height: 100px;
        width: auto;
    }
    
    .Legend {
      grid-column-start: 4;
      grid-column-end: span 9;
      grid-row-start: 1;
      grid-row-end: span 2;
    }
    
    ul.LegendLabels {
      margin: 0;
      padding: 0;
      float: left;
      font-size: 1vw;
      font-weight: bold;
      list-style: none;
      -moz-column-count: 3;
      -moz-column-gap: 1vw;
      -webkit-column-count: 3;
      -webkit-column-gap: 1vw;
      column-count: 3;
      column-gap: 1vw;  
    }
    
    ul.LegendLabels li span {
      display: block;
      float: left;
      height: .8vw;
      width: .8vw;
      margin-right: .5vw;
      margin-left: 0;
    }
    
    .Service {
      width: 4vw;
      height: 4vw;
      background-color: #000;
      color: #fff;
      border-radius: .3vw;
      border: .15vw solid;
      text-align: center;
    }
    
    .Prefix {
      margin: 2px;
      font-size: .6vw;
      width: 100%;
      height: 10%;
      text-align: center;
      color: #e5e5e5;  
    }
    
    .Symbol {
      margin: 4px;
      font-size: 2.5vw;
      height: 50%;
      font-weight: bold;
    }
    
    .Name {
      margin: 4px;
      font-size: .9vw;
      height: 40%;
      text-align: center;
      color: #e5e5e5;
    }
    
    .SmallName {
      font-size: .6vw;
    }

    .ReallySmallName {
      font-size: .5vw;
    }
    
    {{#categories}}
    .{{class}} { color: {{color}}; border-color: {{color}}; }
    {{/categories}}
    
    .Footer {
      margin-top: 40px;
      padding: 20px 0;
      border-top: 2px solid #f86b00;
      text-align: center;
    }
    
    .FooterContent {
      font-size: 1rem;
      color: #666;
    }
    
    .FooterContent a {
      color: #f86b00;
      text-decoration: none;
      font-weight: bold;
      transition: color 0.3s ease;
    }
    
    .FooterContent a:hover {
      color: #d45a00;
      text-decoration: underline;
    }
    
    .FooterSeparator {
      margin: 0 15px;
      color: #999;
    }
  </style>
  <link href='https://fonts.googleapis.com/css?family=Yanone+Kaffeesatz' rel='stylesheet' type='text/css'>
</head>
<body>
  <div class="Container">
    <div class="Wrapper">
      <div class="Title">
          <a href="https://www.tecracer.com/" target="_blank" rel="noopener noreferrer">
            <img src="img/tecracer_logo_rakete.png" alt="TecRacer Logo">
          </a>
          <span>Periodic Table of Amazon Web Services</span>
      </div>
      <div class="Grid">
        <div class="Legend">
          <ul class="LegendLabels">
            {{#categories}}
            <li><span style='background: {{color}};'></span>{{name}}</li>
            {{/categories}}
          </ul>
        </div>
        {{#categories}}
        {{#services}}
        <div class="Service {{category}}" style="grid-column: {{column}}; grid-row: {{row}};" >
          <a href="{{{link}}}" {{#google-analytics}}onclick="trackOutboundLink('{{{link}}}'); return false;"{{/google-analytics}} target="_blank">
            <div class="Prefix">{{prefix}}</div>
            <div class="Symbol {{category}}">{{symbol}}</div>
            <div class="Name {{#long}}SmallName{{/long}} {{#reallong}}ReallySmallName{{/reallong}}">{{name}}</div>
          </a>
        </div>
        {{/services}}
        {{/categories}}
      </div>
      <footer class="Footer">
        <div class="FooterContent">
          <a href="https://www.tecracer.com/impressum/" target="_blank">Impressum</a>
          <span class="FooterSeparator">|</span>
          <a href="https://www.tecracer.com/datenschutz/" target="_blank">Datenschutz</a>
        </div>
      </footer>
    </div>
  </div>
</body>
</html>
